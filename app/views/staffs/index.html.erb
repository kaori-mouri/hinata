<div class="main staffs-index">
    
  <div class="container">
    <a href= "/signup"><h2 class="users-heading">スタッフ新規登録</h2></a>
　</div>
      
    
  <div class="container">
    <h1 class="users-heading">スタッフ一覧</h1>
    <!-- 以下の<% %>を使ってeach文を追加してください -->
    <% @staffs.each do |staff| %>
      <div class="staffs-index-item">
        <div class="staff-right">
          <%= link_to(staff.name, "/staffs/#{staff.id}") %>
        </div>
      </div>
    <!-- 以下の<% %>を使ってeach文のendを追加してください -->
    <% end %>
  </div>
  

</div>
